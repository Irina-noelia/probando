<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head') %>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="ie=edge">
        <title>Helártico</title>
        <script src="https://use.fontawesome.com/releases/v5.15.4/js/all.js" ></script>
        <link rel="stylesheet" href="path/to/font-awesome/css/font-awesome.min.css">
        <script src="https://kit.fontawesome.com/278943c891.js" crossorigin="anonymous"></script>
        <link rel="stylesheet" href="/css/estilo1.css" >
    </head>

    <body>
      <%- include('./partials/header') %>

        <section id="product1" class="section-p1">
            <h2>Tienda</h2>
            
            <div class="pro-container">
                <% products.forEach( prod => { %>
                <div class="pro">
                    <img src="images/<%= prod.image %>" alt="" >
                    <div class="des">
                        <h5> <%= prod.name %> </h5>
                        <span><%= prod.description %></span>
                        <h4> <%= prod.price %> </h4>
                    </div>
                <div class="cart">
                    <a href="/pedido" class="btn btn-info m-1" ><i class="fa-solid fa-cart-shopping"></i>  </a>
              
            </div>
                </div>
                <% } ) %>

        </section>
        
    </body>

    
    <footer class="footer-distributed">

        <div class="footer-left">
  
          <h3>Hel<span>Artico</span></h3>
  
          <p class="footer-links">
            <a href="#">Home</a>
            ·
            <a href="#">Nosotros</a>
            ·
            <a href="#">Shop</a>
            ·
            <a href="#">Contacto</a>
            ·
          </p>
  
          <p class="footer-company-name">Helártico © 2015</p>
  
          <div class="footer-icons">
  
            <a href="#"><i class="fa fa-facebook"></i></a>
            <a href="#"><i class="fa fa-twitter"></i></a>
            <a href="#"><i class="fa fa-linkedin"></i></a>
            <a href="#"><i class="fa fa-github"></i></a>
  
          </div>
  
        </div>
  
        
      </footer>

</html>